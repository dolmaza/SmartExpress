@{
    var layoutViewModel = ViewBag.LayoutViewModel as LayoutViewModel;
    layoutViewModel = layoutViewModel ?? new LayoutViewModel();
}
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title</title>

    <link href="~/Content/plugins/bootstrap/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/plugins/metisMenu/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/sb-admin/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Content/plugins/font-awsome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <link href="~/Areas/Admin/Content/css/style.css" rel="stylesheet" />

    <script src="~/Content/plugins/jquery/jquery-2.2.1.min.js"></script>

    @RenderSection("HeaderSection", false)

</head>

<body>

    <div id="wrapper">
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@layoutViewModel.DashboardUrl">Smart Express</a>
            </div>
            <ul class="nav navbar-top-links navbar-right">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i>  <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="@layoutViewModel.HomeUrl"><i class="fa fa-globe fa-fw"></i> ვებ გვერდზე დაბრუნება</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="@layoutViewModel.LogoutUrl"><i class="fa fa-sign-out fa-fw"></i> გასვლა</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="@layoutViewModel.DictionariesUrl"><i class="fa fa-table fa-fw"></i> ზოგადი ცნობარები</a>
                        </li>

                        <li>
                            <a href="@layoutViewModel.UsersUrl"><i class="fa fa-users fa-fw"></i> მომხმარებლები</a>
                        </li>
                        <li>
                            <a href="@layoutViewModel.InvoicesUrl"><i class="fa fa-list fa-fw"></i> ზედნადებების ისტორია</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="page-wrapper">
            @RenderSection("Breadcrumb", false)
            <div class="row">
                <div class="col-md-12">
                    @if (layoutViewModel?.SuccessErrorMessageInfo != null)
                    {
                        @Html.Partial("_SuccessErrorMessagePartial", layoutViewModel.SuccessErrorMessageInfo)

                    }
                </div>
            </div>

            @RenderBody()
        </div>
    </div>
    <script src="~/Content/plugins/bootstrap/bootstrap.min.js"></script>
    <script src="~/Content/plugins/metisMenu/metisMenu.min.js"></script>
    <script src="~/Content/plugins/sb-admin/sb-admin-2.js"></script>


    @RenderSection("FooterSection", false)
</body>

</html>
