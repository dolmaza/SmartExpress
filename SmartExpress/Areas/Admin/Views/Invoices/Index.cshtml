@model InvoiceViewModel
@{
    ViewBag.Title = "ზედნადებების ისტორია";
}

@section Breadcrumb {
    <div class="row">
        <div class="col-md-12">
            <ol class="breadcrumb">
                <li class="active">@ViewBag.Title</li>
            </ol>
        </div>
    </div>

}

<div class="row">
    <div class="col-md-12">
        <div class="actions-bar">
            <a href="@Model.InvoiceCreateUrl" class="btn btn-default"><i class="fa fa-plus"></i> დამატება</a>
        </div>
    </div>
</div>
